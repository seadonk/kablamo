{
  "version": 2,
  "projects": {
    "canvas": "apps/canvas",
    "canvas-e2e": "apps/canvas-e2e",
    "demo": "apps/demo",
    "demo-e2e": "apps/demo-e2e",
    "drawing": "libs/drawing",
    "four-fours": "apps/four-fours",
    "four-fours-e2e": "apps/four-fours-e2e",
    "mastermind": "libs/mastermind",
    "sudoku": "libs/sudoku",
    "sudoku-ang": "apps/sudoku-ang",
    "sudoku-ang-e2e": "apps/sudoku-ang-e2e",
    "utils": "libs/utils"
  },
  "npmScope": "kablamo",
  "affected": {
    "defaultBase": "main"
  },
  "implicitDependencies": {
    "package.json": {
      "dependencies": "*",
      "devDependencies": "*"
    },
    ".eslintrc.json": "*"
  },
  "tasksRunnerOptions": {
    "default": {
      "runner": "@nrwl/workspace/tasks-runners/default",
      "options": {
        "cacheableOperations": ["build", "lint", "test", "e2e"]
      }
    }
  },
  "targetDependencies": {
    "build": [
      {
        "target": "build",
        "projects": "dependencies"
      }
    ]
  }
}
